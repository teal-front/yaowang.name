{
  "apps": [
    {
      "name": "app",
      "cwd": "/data/main",
      "script": "app.js",
      "instances": 1,
      "exec_mode": "fork",
      "max_memory_restart": "500M",
      "max_restarts": 10,
      "error_file" : "/dev/null",
      "log_file"   : "/dev/null",
      "out_file"   : "/dev/null",
      "merge_logs" : true,
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z"
    }
  ]
}
